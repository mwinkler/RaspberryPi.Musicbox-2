{
  "name": "musicbox",
  "version": "1.0.0",
  "description": "musicbox electron app",
  "main": "build/main.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && npm run build-frontend && npm run build-backend",
    "build-watch": "concurrently \"npm run build-frontend-watch\" \"npm run build-backend-watch\"",
    "build-frontend": "webpack --config webpack.frontend.config.js",
    "build-frontend-watch": "webpack --config webpack.frontend.config.js --watch",
    "build-backend": "webpack --config webpack.backend.config.js",
    "build-backend-watch": "webpack --config webpack.backend.config.js --watch",
    "start": "electron . --local",
    "start-debug": "electron . --local --debug --enable-logging",
    "publish": "electron-packager ./build --out=dist --platform=linux --overwrite --arch=armv7l"
  },
  "dependencies": {
    "@types/electron": "^1.4.35",
    "@types/node": "^7.0.12",
    "@types/react": "^15.0.18",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^4.4.39",
    "font-awesome": "^4.7.0",
    "moment": "^2.18.1",
    "mpd": "^1.3.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "electron": "^1.4.13",
    "electron-packager": "^8.6.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.0",
    "typescript": "^2.1.6",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.3.0"
  },
  "author": "",
  "license": "MIT"
}
